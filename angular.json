{
  "$schema": "./node_modules/@angular-devkit/core/src/workspace/workspace-schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "template-appv6": {
      "root": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "dist/browser",
            "index": "index.html",
            "main": "main.ts",
            "tsConfig": "tsconfig.app.json",
            "polyfills": "polyfills.ts",
            "assets": [
              {
                "glob": "**/*",
                "input": "assets",
                "output": "/assets"
              },
              {
                "glob": "favicon.ico",
                "input": "",
                "output": "/"
              }
            ],
            "styles": [
              "src/assets/styles/bootstrap.css",
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/sweetalert2/dist/sweetalert2.css",
              "src/assets/styles/styles.css",
              "src/assets/styles/newstyle.css",
              "src/assets/styles/chosen/bootstrap-chosen.css",
              "src/assets/styles/iCheck/custom.css",
              "src/assets/styles/animate.css",
              "node_modules/owl.carousel/dist/assets/owl.carousel.css",
              "node_modules/owl.carousel/dist/assets/owl.theme.default.css",
              "src/assets/styles/c3.css",
              "src/assets/styles/clockpicker/clockpicker.css",
              "src/assets/styles/steps/jquery.steps.css",
              "src/assets/styles/jasny/jasny-bootstrap.min.css",
              "node_modules/ng2-toastr/bundles/ng2-toastr.min.css",
              "node_modules/ladda/dist/ladda.min.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "node_modules/metismenu/dist/metisMenu.js",
              "node_modules/sweetalert2/dist/sweetalert2.js",
              "src/vendor/pace/pace.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.js",
              "src/assets/js/d3.js",
              "src/assets/js/c3.js",
              "src/assets/js/hammer.js",
              "src/assets/js/chosen/chosen.jquery.js",
              "src/assets/js/iCheck/icheck.min.js",
              "src/assets/js/clockpicker/clockpicker.js",
              "src/assets/js/jasny/jasny-bootstrap.min.js",
              "src/assets/js/touchspin/jquery.bootstrap-touchspin.min.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "environments/environment.ts",
                  "with": "environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "template-appv6:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "template-appv6:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "template-appv6:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "src/styles.css"
            ],
            "assets": [
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets"
              },
              {
                "glob": "favicon.ico",
                "input": "src",
                "output": "/"
              }
            ]
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/server",
            "main": "src/main.server.ts",
            "tsConfig": "src/tsconfig.server.json"
          }
        }
      }
    },
    "template-appv6-e2e": {
      "root": "",
      "projectType": "application",
      "cli": {},
      "schematics": {},
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "template-appv6:serve"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  },
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "style": "css"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}